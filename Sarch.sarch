architecture { 
        
 
        software_system: Grader; 
        author: Etwo; 
 
        architectural_views {
 
                component_and_connector_view ::


                        elements {


                                programming_languages {
                                                java;
                                                python;
                                                js;
                                                golang;
                                                rubi;
                                                c#;
                                                
                                }


                                db_systems {
                                        mysql;
                                        firebase;
                                        mongodb;
                                }


                                component_types {
                                        frontend;
                                        backend;
                                        database;
                                }
                                
                                component backend gestionarSalones (programming_language golang);
                                component backend gestionarCursos (programming_language C#);
                                component database cursosysalonesdb (db_system mongodb);
                                component database ss_db (db_system firebase);
                                
connector rest;
                                connector mongodb-driver;
                                connector PythonSQL;
                                connector GraphQL;
                        }


                        relations {
                                
attachment (mongodb-driver: gestionarCursos , mongodb);
                                attachment (mongodb-driver: gestionarSalones , mongodb);
                                
                                attachment (GraphQL: ss_web, ss_be);
                                attachment (GraphQL: ss_mobile, ss_be);
                                attachment (PythonSQL: ss_be, ss_db);
                                attachment (PythonSQL: ss_db, ss_be);
                                attachment (rest: ss_be, ss_auth);
                        }
                ::
        }
