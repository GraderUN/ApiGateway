                                component_types { 
                                        frontend; 
                                        backend; 
                                        database; 
                                } 
 
                                component frontend ss_fe (programming_language js); 
                                component backend ss_be (programming_language java); 
                                component database ss_db (db_system mysql); 
                                connector rest; 
                                connector jdbc; 
                        } 
 
                        relations { 
                                attachment (rest: ss_fe, ss_be); 
                                attachment (jdbc: ss_be, ss_db); 
                        } 
                        
